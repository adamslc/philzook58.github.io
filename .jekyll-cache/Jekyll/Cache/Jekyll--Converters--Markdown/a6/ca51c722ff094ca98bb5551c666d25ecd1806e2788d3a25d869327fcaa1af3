<span class="n">fmove'</span> <span class="p">(</span><span class="kt">TTT</span> <span class="p">(</span> <span class="kt">TIT</span>  <span class="n">a</span> <span class="n">b</span><span class="p">)</span> <span class="n">c</span> <span class="p">)</span> <span class="o">=</span> <span class="n">pure</span> <span class="o">$</span> <span class="kt">TIT</span>  <span class="n">a</span>  <span class="p">(</span><span class="kt">TTT</span> <span class="n">b</span> <span class="n">c</span><span class="p">)</span>
<span class="n">fmove'</span> <span class="p">(</span><span class="kt">TTT</span> <span class="p">(</span> <span class="kt">TTT</span>  <span class="n">a</span> <span class="n">b</span><span class="p">)</span> <span class="n">c</span><span class="p">)</span> <span class="o">=</span> <span class="kt">W</span> <span class="p">[(</span><span class="kt">TTI</span>  <span class="n">a</span>  <span class="p">(</span><span class="kt">ITT</span> <span class="n">b</span> <span class="n">c</span><span class="p">),</span> <span class="n">sqrt</span> <span class="n">tau</span><span class="p">)</span>  <span class="p">,</span> <span class="p">(</span><span class="kt">TTT</span>  <span class="n">a</span>  <span class="p">(</span><span class="kt">TTT</span> <span class="n">b</span> <span class="n">c</span><span class="p">),</span>   <span class="o">-</span> <span class="n">tau</span>  <span class="p">)]</span>


<span class="n">fmove'</span> <span class="p">(</span><span class="kt">TIT</span> <span class="p">(</span> <span class="kt">ITT</span>  <span class="n">a</span> <span class="n">b</span><span class="p">)</span> <span class="n">c</span><span class="p">)</span> <span class="o">=</span> <span class="kt">W</span> <span class="p">[(</span><span class="kt">TTI</span>  <span class="n">a</span>  <span class="p">(</span><span class="kt">ITT</span> <span class="n">b</span> <span class="n">c</span><span class="p">),</span> <span class="n">tau</span><span class="p">)</span>         <span class="p">,</span> <span class="p">(</span><span class="kt">TTT</span>  <span class="n">a</span>  <span class="p">(</span><span class="kt">TTT</span> <span class="n">b</span> <span class="n">c</span><span class="p">)</span> <span class="p">,</span> <span class="n">sqrt</span> <span class="n">tau</span><span class="p">)]</span>
