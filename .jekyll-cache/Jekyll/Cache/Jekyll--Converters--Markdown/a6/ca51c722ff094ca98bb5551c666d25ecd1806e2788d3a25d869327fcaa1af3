<span class="kr">type</span> <span class="kt">P</span> <span class="n">a</span> <span class="o">=</span> <span class="kt">W</span> <span class="kt">Double</span> <span class="n">a</span>

<span class="kr">type</span> <span class="kt">Q</span> <span class="n">a</span> <span class="o">=</span> <span class="kt">W</span> <span class="p">(</span><span class="kt">Complex</span> <span class="kt">Double</span><span class="p">)</span> <span class="n">a</span>
