<span class="n">rmap</span> <span class="n">f</span> <span class="p">(</span><span class="kt">TTT</span> <span class="n">l</span> <span class="n">r</span><span class="p">)</span> <span class="o">=</span> <span class="n">fmap</span> <span class="p">(</span><span class="nf">\</span><span class="n">r'</span> <span class="o">-&gt;</span> <span class="kt">TTT</span> <span class="n">l</span> <span class="n">r'</span><span class="p">)</span> <span class="p">(</span><span class="n">f</span> <span class="n">r</span><span class="p">)</span>
